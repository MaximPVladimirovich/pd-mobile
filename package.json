    "redux-thunk": "^2.3.0",
    "yarn": "^1.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@types/aws4": "^1.5.1",
    "@types/react": "^16.8.3",
    "@types/react-native": "~0.57.64",
    "@types/react-native-material-dropdown": "^0.11.0",
    "@types/react-native-vector-icons": "^4.4.3",
