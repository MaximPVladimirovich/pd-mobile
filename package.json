{
  "name": "PoolDash",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run-script copy-assets && node --max-old-space-size=8192 node_modules/react-native/local-cli/cli.js start",
    "copy-assets": "copyfiles -u 1 'src/**/!(*.jsx|*.map|*.ts|*.tsx)' lib/",
    "lint": "tslint 'src/**/*.ts?(x)'",
    "build": "tsc && npm run copy-assets"
  },
  "dependencies": {
