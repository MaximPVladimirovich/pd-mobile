{
  "name": "PoolDash",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run-script copy-assets && node --max-old-space-size=8192 node_modules/react-native/local-cli/cli.js start",
    "copy-assets": "copyfiles -u 1 'src/**/!(*.jsx|*.map|*.ts|*.tsx)' lib/",
    "lint": "tslint 'src/**/*.ts?(x)'",
    "build": "tsc && npm run copy-assets"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^3.0.7",
    "babel-plugin-module-resolver": "^3.1.3",
    "moment": "^2.24.0",
    "prop-types": "^15.0.0",
    "react": "16.6.3",
    "react-native": "0.57.8",
    "react-native-config": "^0.11.7",
    "react-native-keyboard-aware-scroll-view": "^0.8.0",
    "react-native-linear-gradient": "^2.5.3",
    "react-native-material-dropdown": "^0.11.1",
    "react-native-screens": "^1.0.0-alpha.21",
    "react-native-swipeout": "^2.3.3",
    "react-native-touchable-scale": "^2.0.0",
    "react-native-vector-icons": "^4.4.3",
    "react-navigation": "^2.0.0",
    "react-navigation-fluid-transitions": "^0.2.74",
    "react-redux": "^5.0.6",
    "realm": "^2.21.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@types/react": "^16.4.18",
    "@types/react-native": "~0.57",
    "@types/react-native-material-dropdown": "^0.11.0",
    "@types/react-native-vector-icons": "^4.4.2",
    "@types/react-navigation": "^2.0.0",
    "@types/react-redux": "^6.0.12",
    "copyfiles": "^2.1.0",
    "metro-react-native-babel-preset": "^0.49.0",
    "rimraf": "^2.6.2",
    "typescript": "~2.8"
  }
}
